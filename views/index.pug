extends layout.pug
block content
  a(href="/") home
  a(href="/create") create
  a(href="/create-interesting") Create Interesting
  h2 Existing Users
  table
    thead
      tr
        th id
        th First Name
        th Last Name
        th Email
        th Age
        th Favorite Beatle
        th Likes Ice Cream
    tbody
      each user in users
        tr
          td #{user.id}
          td #{user.firstName}
          td #{user.lastName}
          td #{user.email}
          td #{user.age}
          td #{user.favoriteBeetle}
          td #{user.likesIceCream}


